  transition: left 0.7s ease;
}

.status-card:hover, .system-card:hover {
  transform: translateY(-5px);
  box-shadow: 0 15px 30px rgba(156, 77, 255, 0.2);
  border-color: rgba(156, 77, 255, 0.3);
}

.status-card:hover::after, .system-card:hover::after {
  left: 100%;
}

.progress-bar {
