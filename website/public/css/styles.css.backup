/* Command Cards Enhancement */
.command-card {
  position: relative;
  overflow: hidden;
  border: 1px solid rgba(156, 77, 255, 0.1);
  box-shadow: 0 8px 15px rgba(0, 0, 0, 0.2);
  transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
}

.command-card::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 5px;
  background: linear-gradient(90deg, #9c4dff, #c792ff);
  transform: scaleX(0);
  transform-origin: left;
  transition: transform 0.4s ease;
}

.command-card:hover {
  transform: translateY(-10px);
  box-shadow: 0 15px 30px rgba(156, 77, 255, 0.2);
  border-color: rgba(156, 77, 255, 0.3);
}

.command-card:hover::before {
  transform: scaleX(1);
}

.command-name {
  position: relative;
  display: inline-block;
  margin-bottom: 15px;
  padding-bottom: 5px;
}

.command-name::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  width: 30px;
  height: 2px;
  background-color: var(--primary-color);
  transition: width 0.3s ease;
}

.command-card:hover .command-name::after {
  width: 100%;
}

/* Status Cards Enhancement */
.status-card, .system-card {
  border: 1px solid rgba(156, 77, 255, 0.1);
  box-shadow: 0 8px 15px rgba(0, 0, 0, 0.2);
  transition: all 0.3s ease;
  overflow: hidden;
  position: relative;
}

.status-card::after, .system-card::after {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, 
    transparent, 
    rgba(156, 77, 255, 0.05), 
