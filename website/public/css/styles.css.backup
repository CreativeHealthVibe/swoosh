/* Hero Section Enhancement */
.hero {
  position: relative;
  min-height: 80vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  overflow: hidden;
  background: linear-gradient(180deg, rgba(13,13,13,1) 0%, rgba(25,25,25,1) 100%);
}

.hero-content {
  position: relative;
  z-index: 1;
  text-align: center;
  padding: 2rem;
}

.hero h1 {
  font-size: 4.5rem;
  font-weight: 800;
  margin-bottom: 1.5rem;
  background-color: #9c4dff;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  text-shadow: 0 0 25px rgba(156, 77, 255, 0.3);
  letter-spacing: -2px;
  animation: fadeIn 1s ease-out;
  transform: translateY(0);
  transition: transform 0.3s ease-out;
}

.hero p {
  font-size: 1.5rem;
  max-width: 700px;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 2rem;
  opacity: 0.9;
  animation: fadeIn 1.5s ease-out;
}

.hero .btn {
  animation: fadeIn 2s ease-out;
  position: relative;
  overflow: hidden;
  transition: all 0.5s ease;
  z-index: 1;
}

.hero .btn:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(45deg, #9c4dff, #c792ff);
  opacity: 0;
  transition: all 0.5s ease;
  z-index: -1;
  transform: translateY(100%);
}

.hero .btn:hover:before {
  opacity: 1;
  transform: translateY(0);
}

/* Scroll Indicator */
.scroll-down {
  position: absolute;
  bottom: 30px;
  left: 50%;
  transform: translateX(-50%);
  width: 30px;
  height: 50px;
  border: 2px solid rgba(156, 77, 255, 0.7);
  border-radius: 15px;
  cursor: pointer;
  z-index: 2;
  opacity: 0;
  animation: fadeIn 2.5s ease-out forwards;
}

.scroll-down:after {
  content: '';
  position: absolute;
  top: 8px;
  left: 50%;
  width: 6px;
  height: 6px;
  margin-left: -3px;
  background-color: #9c4dff;
  border-radius: 50%;
  animation: scrollMove 2s infinite;
}

@keyframes scrollMove {
  0% {
    opacity: 0;
    top: 8px;
  }
  40% {
    opacity: 1;
  }
  80% {
    opacity: 0;
    top: 32px;
  }
  100% {
    opacity: 0;
    top: 8px;
  }
}

@keyframes fadeIn {
  from {
    opacity: 0;
    transform: translateY(20px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

